{
  "name": "react-integration-examples",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:with-react-craco": "lerna run start --scope with-react-craco",
    "start:with-react-vite": "lerna run start --scope with-react-vite",
    "start:with-nextjs": "lerna run start --scope with-nextjs",
    "start:all": "lerna run start --parallel",
    "build:with-react-craco": "lerna run build --scope with-react-craco",
    "build:with-react-vite": "lerna run build --scope with-react-vite",
    "build:with-nextjs": "lerna run build --scope with-nextjs",
    "build:all": "lerna run build --parallel",
    "clean-install": "find . -name 'node_modules' -type d -prune -exec rm -rf {} + && find . -name '.cache' -type d -prune -exec rm -rf {} + && yarn install"
  },
  "devDependencies": {
    "lerna": "^8.1.3"
  }
}
