{
  "name": "react-integration-examples",
  "packageManager": "yarn@1.22.22",
  "private": true,
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "start:react-app": "lerna run start --scope react-app",
    "start:vite-app": "lerna run dev --scope vite-app",
    "start:nextjs-app": "lerna run dev --scope nextjs-app",
    "start:pwa-app": "lerna run start --scope pwa-app",
    "start:remix-app": "lerna run dev --scope remix-app",
    "start:all": "lerna run start --parallel"
  },
  "devDependencies": {
    "lerna": "^8.1.3"
  }
}
